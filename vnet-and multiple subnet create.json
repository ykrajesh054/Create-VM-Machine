                },
                {
                    "name": "Subnet-2",
                    "properties": {
                        "addressPrefix": "10.0.1.0/24"
                    }
                },
                {
                    "name": "Subnet-3",
                    "properties": {
                        "addressPrefix": "10.0.2.0/24"
                    }
                },
                {
                    "name": "Subnet-4",
                    "properties": {
                        "addressPrefix": "10.0.3.0/24"
                    }
                },
                 {
                    "name": "Subnet-5",
                    "properties": {
                        "addressPrefix": "10.0.4.0/28"
                    }
